{
    "manifest_version": 2,
    
    "name": "ACBrewingCalculator",
    "description": "This extension provides an easy-access calculator for brewers",
    "version": "0.1",
    
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "default_title": "AC Brewing Calculator"
    },

    "icons": {
    "16": "icon16.png",
    "128": "icon128.png" },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
